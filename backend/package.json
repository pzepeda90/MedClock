  "description": "API RESTful para sistema de agendamiento m√©dico",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "test": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:working": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=\"(mock.test.js|rutas.simple.test.js|funciones.test.js|integration/cita.simple.test.js)\"",
    "test:watch": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "migrate:pacientes": "node database/run-migration.js",
    "migrate:codigos": "node scripts/migrate-codigos.js",
    "migrate:codigos:samples": "node scripts/migrate-codigos.js --with-samples",
    "migrate:auditoria": "node scripts/migrate-auditoria.js",
    "db:check": "node scripts/check-tables.js",
    "init:db": "node database/init-db.js",
    "init:db:full": "node database/init-full-db.js",
    "db:init:all": "node scripts/init-database.js",
    "db:init:all:samples": "node scripts/init-database.js --with-samples",
    "db:init:force": "node scripts/init-database.js --force",
    "db:init:force:samples": "node scripts/init-database.js --force --with-samples"
  },
  "keywords": [
    "healthcare",
    "scheduling",
