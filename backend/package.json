    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.3",
    "node-fetch": "^3.3.0",
    "pg": "^8.11.3",
    "pg-format": "^1.0.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.1",
